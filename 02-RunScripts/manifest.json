{
    "name": "StatusPlus",
    "description": "Tell the statistics of the current Tic Tac Toe status.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html",
      "default_icon": "logo.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://gemezzz.github.io/Tic-Tac-Toe-Reloaded/*"
            ]
        }
    ]
  }